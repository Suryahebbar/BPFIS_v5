# Database
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/agrilink

# Next.js Auth
NEXTAUTH_URL=https://your-app-name.onrender.com
NEXTAUTH_SECRET=your-secret-key-generate-with-openssl-rand-base64-32

# Blockchain (Development mode by default)
BLOCKCHAIN_MODE=development

# Email Service (for notifications)
EMAIL_SERVICE_USER=your-email@gmail.com
EMAIL_SERVICE_PASS=your-app-password

# Optional: Real Blockchain (if you want to use actual blockchain)
# BLOCKCHAIN_MODE=real
# BLOCKCHAIN_CONTRACT_ADDRESS=
# BLOCKCHAIN_ADMIN_PRIVATE_KEY=

# Other services
NODE_ENV=production
